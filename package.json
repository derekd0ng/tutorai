{
  "name": "tutorai",
  "version": "1.0.0",
  "description": "A webapp for tutors to manage students, courses, and tasks with AI chatbot",
  "main": "server/index.js",
  "scripts": {
    "install-deps": "npm install && cd client && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm run dev",
    "start": "node server/index.js"
  },
  "keywords": ["tutor", "education", "react", "express", "ai"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  }
}